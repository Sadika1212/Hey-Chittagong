            </div>
        </div>



    </section>

    <script src="../jq/jquery.js" type="text/javascript"></script>
    <script src="../js/bootstrap.js" type="text/javascript"></script>
    <script src="script.js"></script>
    <script>
            $(function () {
        $(".openB").click(function () {
            $(".first").addClass("open");
            setTimeout(function () {
                $(".right").addClass("open");
            }, 250);
            setTimeout(function () {
                $(".back").addClass("open");
            }, 350);
            $(".closeB").delay(400).fadeIn();
        });

        $(".closeB").click(function () {
            setTimeout(function () {
                $(".first").removeClass("open");
            }, 250);
            $(".right").removeClass("open");
            setTimeout(function () {
                $(".back").removeClass("open");
            }, 600);
            $(".closeB").fadeOut();
        });
    });
    </script>

</body>

