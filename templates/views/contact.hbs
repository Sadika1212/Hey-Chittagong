    <section class="contact">
        <h2>Contact Us</h2>
        <div class="wrapper">
            <div class="first">
                <div class="front">
                    <div class="sectionWrap">
                        <h4>get in touch</h4>
                        <br>
                        <p class="preview">Lorem ipsum dolor sit amet, consetetur sadips Lorem ipsum dolor sit amet,
                            consetetur sadipscing elitr,onsetetur sadipscing elitr, sed diam nonumy eirmod
                        </p>
                        <button class="openB">contact now</button>
                    </div>
                </div>
                <div class="back">
                    <div class="sectionWrap">
                        <div class="contactinfo">
                            <h4>contact info</h4>
                            <ul class="info">
                                <li>
                                    <span><i class="fa fa-map-marker"></i></span>
                                    <span>Chadgaon,Chittagong<br>Bangladesh</span>
                                </li>
                                <li>
                                    <span><i class="fa fa-envelope"></i></span>
                                    <span class="mail">sadikaafroze00001@<br>
                                        gmail.com</span>
                                </li>
                                <li>
                                    <span><i class="fa fa-phone"></i></span>
                                    <span>+8801627043316</span>
                                </li>

                            </ul>
                            <ul class="sci">
                                <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                                <li><a href="#"><i class="fa fa-pinterest"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="middle">
                <div class="sectionWrap">
                    <form action="/contact" method="POST" class="contact-form">
                        <h4>write to us</h4>
                        <div class="form-group"><input class="form-control" type="text" name="name" required=""
                                placeholder="Name">
                        </div>
                        <div class="form-group"><input class="form-control" type="email" name="email" required=""
                                placeholder="Email">
                        </div>
                        <div class="form-group"><textarea name="message" placeholder="Message" cols="30"
                                rows="4"></textarea></div>
                        <button type="submit" class="btn sub">Send Message</button>
                    </form>
                </div>
            </div>
            <div class="right">
